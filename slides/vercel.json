{
  "outputDirectory": "dist",
  "cleanUrls": false,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/coding-as-transformation/:path*",
      "destination": "/coding-as-transformation/index.html"
    },
    {
      "source": "/socratic-programmer/:path*",
      "destination": "/socratic-programmer/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}